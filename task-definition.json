{
  "family": "simple-web-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::9054-1817-9651:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::9054-1817-9651:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "simple-web-container",
      "image": "905418179651.dkr.ecr.ap-southeast-1.amazonaws.com/simple-web:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/simple-web",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
